Algoritmo factorial5
	Definir factorial Como entero;
	factorial<-1;
    Escribir "Programa que calcula factorial desde 1! hasta 5!: ";	
    
    Para i<-1 Hasta 5 Con Paso 1 Hacer
		factorial<-factorial*i;
		
		Para j<-1 hasta 1 con paso 1 Hacer
			Escribir "El factorial de ",i,"! es ",factorial;
		FinPara
    FinPara
    
    
FinAlgoritmo
